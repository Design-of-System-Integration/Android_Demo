<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cur_cur"
    >

    <Button
        android:id="@+id/btn_playvideo"
        android:layout_width="151dp"
        android:layout_height="106dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="32dp"
        app:backgroundTint="#00FFFFFF"
        style="@style/Widget.AppCompat.Button.Borderless"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_addvideo"
        android:layout_width="141dp"
        android:layout_height="97dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="252dp"
        app:backgroundTint="#00FFFFFF"
        style="@style/Widget.AppCompat.Button.Borderless"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_cur_in_req"
        android:layout_width="61dp"
        android:layout_height="49dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="252dp"
        app:backgroundTint="#00FFFFFF"
        style="@style/Widget.AppCompat.Button.Borderless"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_cur_in_storage"
        android:layout_width="84dp"
        android:layout_height="46dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="152dp"
        app:backgroundTint="#00FFFFFF"
        style="@style/Widget.AppCompat.Button.Borderless"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_sto_in_his"
        android:layout_width="211dp"
        android:layout_height="71dp"
        app:backgroundTint="#00FFFFFF"
        style="@style/Widget.AppCompat.Button.Borderless"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"

        />

    <Button
        android:id="@+id/btn_insurance"
        android:layout_width="199dp"
        android:layout_height="64dp"
        app:backgroundTint="#00FFFFFF"
        style="@style/Widget.AppCompat.Button.Borderless"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
         />
    <Button
        android:id="@+id/btn_home"
        android:layout_width="50dp"
        android:layout_height="46dp"
        android:layout_marginTop="29dp"
        app:backgroundTint="#00FFFFFF"
        style="@style/Widget.AppCompat.Button.Borderless"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>